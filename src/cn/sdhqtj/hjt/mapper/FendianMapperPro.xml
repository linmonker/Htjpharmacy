<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.sdhqtj.hjt.mapper.FendianMapperPro">

	<select id="fendianquery" resultType="cn.sdhqtj.hjt.entity.Fendian">
		select
		*
		from
		hjt_fendian
		where zt=0
	</select>
	<select id="selectByfdbh" resultType="cn.sdhqtj.hjt.entity.Fendian"
		parameterType="java.lang.String">
		select
		*
		from
		hjt_fendian
		where
		fdbh=#{_parameter} and zt=0
	</select>
	<select id="selectByfdmc" resultType="cn.sdhqtj.hjt.entity.Fendian"
		parameterType="java.lang.String">
		select
		*
		from
		hjt_fendian
		where
		fdmc=#{_parameter} and zt=0
	</select>
</mapper>