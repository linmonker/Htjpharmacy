<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.sdhqtj.hjt.mapper.LoginMapper">

	<select id="login" resultType="cn.sdhqtj.hjt.entity.Login">
		select
		*
		from
		hjt_yonghu
		where
		yhbh=#{login}
		and
		yhmm=#{password}
	</select>

</mapper>